<?xml version="1.0" encoding="utf-8"?>
<!-- 带圆角 白色背景 黄色边框 长方体 -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>

            <solid android:color="#FFFFFF"/>
            <corners android:topLeftRadius="1dp" android:topRightRadius="1dp"
                android:bottomRightRadius="1dp" android:bottomLeftRadius="1dp"/>
            <stroke android:width="1dp" android:color="#cfb166"/>
            <padding
                android:left="5dp"
                android:top="5dp"
                android:right="5dp"
                android:bottom="5dp"
                />
        </shape>
    </item>

</layer-list>