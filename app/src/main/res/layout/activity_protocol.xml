<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_protocol"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tiemuyu.chuanchuan.activity.ProtocolActivity">
    <RelativeLayout
        android:id="@+id/head"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/address_tishi">

        <ImageView
            android:id="@+id/im_back"
            android:layout_width="50dp"
            android:layout_height="25dp"
            android:layout_marginBottom="11dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="11dp"
            android:src="@drawable/home_back" />

        <TextView
            android:gravity="center"
            android:layout_marginRight="60dp"
            android:layout_marginLeft="10dp"
            android:lines="1"
            android:id="@+id/tv_head"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="协议内容"
            android:textColor="@android:color/black"
            android:textSize="18dp"
            android:layout_alignBottom="@+id/im_back"
            android:layout_toEndOf="@+id/im_back" />
        <ImageView
            android:id="@+id/im_share"
            android:background="@drawable/nav_share"
            android:layout_marginRight="20dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_width="25dp"
            android:layout_height="25dp" />
    </RelativeLayout>
    <WebView
        android:layout_below="@id/head"
        android:id="@+id/web_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</RelativeLayout>
